  if (this.isMobile()) {\r\n      this.$store.dispatch(\"toggleDevice\", \"mobile\");\r\n      this.$store.dispatch(\"closeSideBar\");\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style lang=\"css\">\r\n@import url(\"https://fonts.googleapis.com/css?family=Roboto:400,500,500i,700\");\r\nbody {\r\n  font-family: \"Roboto\", sans-serif;\r\n}\r\n</style>\r\n"],"sourceRoot":"src"}]}