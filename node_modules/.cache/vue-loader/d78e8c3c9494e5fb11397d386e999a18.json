te.emailTemplateData;\r\n    },\r\n    role: function() {\r\n      if (this.$store.state.role === \"Admin/管理员\") {\r\n        return true;\r\n      } else {\r\n        return false;\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    updateEmail() {\r\n      this.loading = true;\r\n      this.$store\r\n        .dispatch(\"updateEmailTemplate\", this.postForm)\r\n        .then(() => {\r\n          this.$message.success(\"修改成功！\");\r\n          this.$emit(\"closeDialog\");\r\n          this.loading = false;\r\n          this.$router.push(\"/dashboard/emaillist\");\r\n        })\r\n        .catch(() => {\r\n          this.$message.error(\"修改失败！\");\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.emailContainer {\r\n  width: 95%;\r\n  margin: 0 auto;\r\n  h3 {\r\n    span {\r\n      font-size: 18px;\r\n      color: #000000;\r\n    }\r\n  }\r\n  .emailDisplay {\r\n    font-size: 16px;\r\n  }\r\n}\r\n</style>\r\n"]}]}